{
  "name": "@sanring/gx-drag-drop",
  "version": "0.1.0",
  "description": "Lightweight drag & drop utilities (native + CDK helpers) for Angular",
  "license": "MIT",
  "scripts": {
    "build": "ng-packagr -p ng-package.json"
  },
  "peerDependencies": {
    "@angular/cdk": "^20.1.0",
    "@angular/common": "^20.1.0",
    "@angular/core": "^20.1.0"
  },
  "peerDependenciesMeta": {
    "@angular/cdk": {
      "optional": true
    }
  },
  "dependencies": {
    "tslib": "^2.3.0"
  },
  "devDependencies": {
    "ng-packagr": "^20.1.0",
    "typescript": "^5.9.2"
  },
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/fesm2022/sanring-gx-drag-drop.mjs"
    },
    "./styles/*": {
      "default": "./dist/styles/*"
    }
  },
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
